    "karma-verbose-reporter": "^0.0.6",
    "log4js": "^6.1.2",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "protractor": "~5.4.3",
    "rimraf": "^3.0.2",
    "tailwindcss": "1.2.0",
    "ts-node": "~8.8.1",
    "typescript": "~3.7.5"
  }
