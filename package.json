{
  "name": "angular-tailwind-eslint",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "lint": "ng lint",
    "test:unit": "ng test --browsers=ChromeHeadless --code-coverage",
    "test:e2e": "tsc e2e/protractor.conf.ts && ng e2e",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "coverage": "http-server -c-1 -p 9875 ./coverage",
    "build": "rimraf ./dist && ng build --prod",
    "build:ci": "npm run test:ci && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.0.0",
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular-eslint/builder": "^0.0.1-alpha.27",
    "@angular-eslint/eslint-plugin": "^0.0.1-alpha.27",
    "@angular-eslint/eslint-plugin-template": "^0.0.1-alpha.27",
    "@angular-eslint/template-parser": "^0.0.1-alpha.27",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@types/jasmine": "~3.5.9",
    "@types/jasminewd2": "~2.0.8",
    "@types/karma": "^4.4.0",
    "@types/karma-coverage-istanbul-reporter": "^2.1.0",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "bluebird": "^3.7.2",
    "codelyzer": "^5.2.1",
    "eslint": "^6.8.0",
    "http-server": "^0.12.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
